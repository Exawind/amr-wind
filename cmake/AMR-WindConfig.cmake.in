@PACKAGE_INIT@

list(APPEND CMAKE_MODULE_PATH "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_DATADIR@/cmake/Modules")

set(AMR_WIND_ENABLE_MASA @AMR_WIND_ENABLE_MASA@)
set(AMR_WIND_ENABLE_NETCDF @AMR_WIND_ENABLE_NETCDF@)
set(AMR_WIND_USE_INTERNAL_AMREX @AMR_WIND_USE_INTERNAL_AMREX@)
set(AMR_WIND_USE_INTERNAL_AMREX_HYDRO @AMR_WIND_USE_INTERNAL_AMREX_HYDRO@)
set(AMR_WIND_USE_INTERNAL_WAVES2AMR @AMR_WIND_USE_INTERNAL_WAVES2AMR@)

if (NOT ${AMR_WIND_USE_INTERNAL_AMREX})
  set(AMReX_DIR @AMReX_DIR@)
else()
  set(AMReX_DIR "${CMAKE_CURRENT_LIST_DIR}/../AMReX")
endif()
find_package(AMReX QUIET REQUIRED)

if (NOT ${AMR_WIND_USE_INTERNAL_AMREX_HYDRO})
  set(AMReX-Hydro_DIR @AMReX-Hydro_DIR@)
else()
  set(AMReX-Hydro_DIR "${CMAKE_CURRENT_LIST_DIR}/../AMReX-Hydro")
endif()
find_package(AMReX-Hydro QUIET REQUIRED)

if (NOT ${AMR_WIND_USE_INTERNAL_WAVES2AMR})
  set(Waves2AMR_DIR @Waves2AMR_DIR@)
else()
  set(Waves2AMR_DIR "${CMAKE_CURRENT_LIST_DIR}/../Waves2AMR")
endif()
if (AMR_WIND_ENABLE_W2A)
  find_package(Waves2AMR QUIET REQUIRED)
endif()

if (${AMR_WIND_ENABLE_MASA})
  set(MASA_DIR @MASA_DIR@)
  find_package(MASA QUIET REQUIRED)
endif()

if (${AMR_WIND_ENABLE_NETCDF})
  set(NetCDF_DIR @NETCDF_DIR@)
  set(NETCDF_DIR @NETCDF_DIR@)
  find_package(NetCDF QUIET REQUIRED)
endif()

set(AMR_WIND_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_INCLUDEDIR@")
set(AMR_WIND_LIBRARY_DIRS "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@")

include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

set(AMR_WIND_LIBRARIES "@PROJECT_NAME@::@aw_api_lib@")

set(AMR_WIND_FOUND TRUE)

check_required_components(AMR_WIND)
